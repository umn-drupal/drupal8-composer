  "repositories": {
    "drupal-packagist": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "umn-features": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/d8-features.git"
    },
    "emergency_management": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/emergency_notification.git"
    },
    "lightning_umn": {
      "type": "git",
      "url": "git@github.umn.edu:drupalprofiles/lightning_umn.git"
    },
    "cog-umn": {
      "type": "git",
      "url": "git@github.umn.edu:drupalthemes/cog-umn.git"
    },
    "bootstrap-umn": {
      "type": "git",
      "url": "git@github.umn.edu:drupalthemes/bootstrap-umn.git"
    },
    "umn-search": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/umn-search.git"
    },
    "crmform": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/crmform"
    },
    "dors_migrate": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/dors_migrate.git"
    },
    "umn-superfish": {
      "type": "git",
      "url": "git@github.umn.edu:dtadege/superfish.git"
    },
    "umn_folwell_components": {
      "type": "git",
      "url": "git@github.umn.edu:drupalmodules/folwell_components.git"
    },
    "blazy": {
      "type": "package",
      "package": {
        "name": "dinbror/blazy",
        "type": "drupal-library",
        "version": "1.8.2",
        "source": {
          "url": "https://github.com/dinbror/blazy.git",
          "type": "git",
          "reference": "1.8.2"
        }
      }
    },
    "jquery.easing": {
      "type": "package",
      "package": {
        "name": "gdsmith/easing",
        "type": "drupal-library",
        "version": "1.4.1",
        "source": {
          "url": "https://github.com/gdsmith/jquery.easing.git",
          "type": "git",
          "reference": "1.4.1"
        }
      }
    },
    "fakeobjects": {
      "type": "package",
      "package": {
        "name": "ckeditor/fakeobjects",
        "type": "drupal-library",
        "version": "4.10.1",
        "dist": {
          "url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.10.1.zip",
          "type": "zip"
        }
      }
    },
    "anchor_link": {
      "type": "package",
      "package": {
        "name": "ckeditor/anchor_link",
        "version": "4.10.1",
        "dist": {
          "url": "https://download.ckeditor.com/link/releases/link_4.10.1.zip",
          "type": "zip"
        }
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-module-custom"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-profile-custom"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-theme-custom"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "source": "http://cgit.drupalcode.org/drupal/plain/{path}?h={version}",
      "excludes": [
        "robots.txt",
        "sites/default/default.services.yml",
        "sites/default/default.settings.php"
      ]
    },
    "patches-file": "composer.patches.json",
    "enable-patching": true,
    "symlinks": {
      "vendor/kenwheeler/slick": "docroot/libraries/slick",
      "docroot/libraries/jquery.mmenu": "docroot/libraries/mmenu",
      "vendor/ckeditor/anchor_link": "docroot/libraries/ckeditor/plugins/link"
    }
  }
}
